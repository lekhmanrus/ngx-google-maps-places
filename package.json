    "@types/jest": "^29.5.14",
    "angular-eslint": "18.4.1",
    "conventional-changelog-angular": ">=8.0.0",
    "eslint": "^9.15.0",
    "husky": "^9.1.7",
    "is-ci": ">=3.0.1",
    "jest": "^29.7.0",
    "jest-auto-spies": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "ng-mocks": "^14.13.1",
    "ng-packagr": "^18.2.1",
    "npm-run-all": "^4.1.5",
    "standard-version": ">=9.5.0",
    "typescript": "~5.5.2",
